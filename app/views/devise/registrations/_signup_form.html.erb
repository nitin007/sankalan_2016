<div id="newuser" class="tab-pane fade in active">
  <!--<p>The secret of getting ahead is getting started.</p>-->
  <h4>Register and Get Started</h4>
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: {class: 'reg-form'}) do |f| %>
    <%= devise_error_messages! %>
    <div class="form-group">
      <%= f.label :team_name %>
      <%= f.text_field :team_name, class: 'form-control', minlength: "4", maxlength: "20", size: "20", required: true %>
    </div>
    <div class="form-group">
      <%= f.label :team_email %>
      <%= f.email_field :email, class: 'form-control', size: "20", oninvalid: "setCustomValidity('Please Enter a valid e-mail id')", oninput: "setCustomValidity('')", required: true %>
    </div>
    <div class="form-group">
      <%= f.label :team_contact %>
      <%= f.text_field :contact_no, class: 'form-control', size: "20" %>
    </div>
    <div class="form-group">
      <%= f.label :events %>
      <%= f.select :events, [[1, 2]] class: 'form-control', size: "20" %>
    </div>
    <div class="form-group">
      <%= f.label :password %>
      <% if @minimum_password_length %>
      <em>(<%= @minimum_password_length %> characters minimum)</em>
      <% end %><br />
      <%= f.password_field :password, autocomplete: "off", class: 'form-control', minlength: "6", maxlength: "20", size: "20", required: true %>
    </div>
    <div class="form-group">
      <%= f.label :password_confirmation %><br />
      <%= f.password_field :password_confirmation, autocomplete: "off", class: 'form-control', minlength: "6", maxlength: "20", size: "20", required: true %>
    </div>
    <div class="form-group">
      <%= f.submit "Sign up", value: 'Next', class: 'nomarginbutton btn btn-default', id: 'next_btn', onclick: "return ValidatePassword()" %>
    </div>
  <% end %>
</div>
