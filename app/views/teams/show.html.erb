<div class="teamdetails table-responsive">
  <br />
  <h1 align="center"> Welcome Team <%= @team.team_name %> , Your Details : </h1>
  <br/><br/>
  <table class="table table-striped table-bordered teamtableformat">
    <thead>
    </thead>
    <tbody>
      <tr>
        <td>Team Status : </td>
        <td><%= @team.get_status %></td>
      </tr>
      <tr>
        <td>Number of Members : </td>
        <td><%= @team.members_count %></td>
      </tr>
      <tr>
        <td>Team Captain : </td>
        <td><%= form_for @team, url: change_captain_team_path(@team), method: :post do |f| %>
            <%= f.select :captain, options_from_collection_for_select(@team.members, :id, :name) %>
            <% end %>
        </td>
      </tr>
      <tr>
        <td>Events : </td>
        <td><%= form_for @team, url: change_event_team_path(@team), method: :post do |f| %>
            <%= f.select :captain, events_collection, class:"form-control" %>
            <% end %>
        </td>
      </tr>
       <tr>
        <td>Email : </td>
        <td><%= @team.email %></td>
      </tr>
      <tr>
        <td>Contact : </td>
        <td><%= @team.contact_no %></td>
      </tr>
    </tbody> 
  </table>
  <br />
</div>
<!-- Link to team's members page-->
<div id="centerbutton">
  <a href="#" class="btn btn-white" role="button">Edit Members Info</a>
</div>
 
<br />
<div class="memberdetails">
    <h2 class="currmem"> Current Members </h2>
    <br />
    <div class= "table-responsive">
    	<table class="table table-striped table-bordered memberstableformat">
        <thead>
        	<tr>
          	<td>Full Name</td>
          	<td>College Name</td>
          	<td>Sex</td>
          	<td>E-mail</td>
          	<td>Contact Number</td>
          	<td>Accomodation</td>
        	<tr/>
      </thead>
      <tbody>
        <% @team.members.each do |member| %>
          <tr>
            <td><%= member.name %></td>
          	<td><%= member.college %></td>
          	<td><%= member.gender %></td>
          	<td><%= member.email %></td>
          	<td><%= member.contact_no %></td>
          	<td><%= member.accomodation_required %></td>
          </tr>
        <% end %>
      </tbody>
    	</table>
    </div>
</div>

<% content_for :head do %>
  <%= stylesheet_link_tag "teampage" %>
  <%= stylesheet_link_tag "demo" %>
<% end %>