<h2 align="center"> Registered Team Details Page</h2>
<div class="table-responsive">   
<table class="table table-striped table-bordered">
  <thead>
    <tr class="info">
      <th>Team name</th>
      <th>Email</th>
      <th>Contact no</th>
      <th>No. of members</th>
      <th>Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @teams.each do |team| %>
      <tr>
        <td><%= team.team_name %></td>
        <td><%= team.email %></td>
        <td><%= team.contact_no %></td>
        <td><%= team.members_count %></td>
        <td><%= link_to 'Edit', edit_team_path(team) %></td>
      </tr> 
    <% end %>
  </tbody>
</table>
</div>
