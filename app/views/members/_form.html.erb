<%= form_for([@team, @member]) do |f| %>
  <div class='form-group'>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class='form-group'>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>
  <div class='form-group'>
    <%= f.label :college %>
    <%= f.text_field :college %>
  </div>
  <div class='form-group'>
    <%= f.label :contact_no %>
    <%= f.text_field :contact_no %>
  </div>
  <div class='form-group'>
    <%= f.label :gender %>
    <%= f.select :gender, gender_collection %>
  </div>
  <div class='form-group'>
    <%= f.label :accomodation_needed %>
    <%= f.select :accomodation_needed, accomodation_options %>
  </div>
  <div class='form-group'>
    <%= f.submit (f.object.new_record? ? "Create" : "Update") %>
  </div>
<% end %>
